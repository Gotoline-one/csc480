<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="0" minHeight="0" minWidth="-Infinity" prefHeight="377.0"
      prefWidth="518.0" xmlns="http://javafx.com/javafx/8" fx:controller="csc480.NewScoutController">
    <children>
        <AnchorPane layoutX="10.0" layoutY="10.0" maxHeight="-1.0" maxWidth="-1.0" prefHeight="351.0" prefWidth="494.0">
            <children>
                <TextField fx:id="firstNameTxtBox" layoutX="106.0" layoutY="34.0" prefHeight="25.0" prefWidth="122.0"/>
                <TextField fx:id="lastNameTxtBox" layoutX="231.0" layoutY="34.0" prefHeight="25.0" prefWidth="122.0"/>
