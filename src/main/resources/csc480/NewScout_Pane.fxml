                <Button fx:id="cancelBtn" layoutX="172.0" layoutY="300.0" mnemonicParsing="false" onAction="#cancel"
                        text="Cancel"/>
                <Button fx:id="saveScoutBtn" layoutX="292.0" layoutY="300.0" mnemonicParsing="false"
                        onAction="#saveScout" text="Save"/>
                <TextField fx:id="scoutPosition1" editable="false" focusTraversable="false" layoutX="106.0"
                           layoutY="189.0" prefHeight="25.0" prefWidth="246.0"/>
                <TextField fx:id="scoutPosition11" editable="false" focusTraversable="false" layoutX="106.0"
