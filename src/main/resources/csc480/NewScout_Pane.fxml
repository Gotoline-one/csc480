                <TextField fx:id="scoutRank" layoutX="106.0" layoutY="105.0" prefHeight="25.0" prefWidth="246.0"/>
                <TextField fx:id="scoutPosition" layoutX="106.0" layoutY="141.0" prefHeight="25.0" prefWidth="246.0"/>
                <Label layoutX="22.0" layoutY="146.0" text="Position"/>
                <ImageView fx:id="scoutImage" fitHeight="153.0" fitWidth="184.0" layoutX="353.0" layoutY="41.0"
